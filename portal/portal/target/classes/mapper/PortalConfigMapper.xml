<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.exrick.xboot.modules.portalConfig.dao.mapper.PortalConfigMapper">


    <!--根据key获取配置信息-->
    <select id="selectPortalConfigByKey" resultType="cn.exrick.xboot.modules.portalConfig.entity.PortalConfig" >
        SELECT * FROM t_portal_config where config_key=#{config_key}
    </select>


</mapper>